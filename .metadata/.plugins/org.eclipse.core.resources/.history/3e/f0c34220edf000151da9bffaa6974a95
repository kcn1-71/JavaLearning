
public class BeforeIForget {

	public static void ShellsSort(int[] a) {

		int[] b = { 1, 4, 10, 23 };
		int m = 0;
		while (b[m] < a.length)
			m++;

		int key, j,gap ;

		for (; m >= 0; m--) {
			gap = b[m];
			for (int i = gap; i < a.length; i++) {
				j = i;
				key = a[j];
				while (j >= gap && a[j - gap] > key) {
					a[j] = a[j - gap];
					j -= gap;
				}
				a[j] = key;
			}
		}
	}

	public static void main(String[] args) {
		int[] A = new int[] { 2, 4, 1, 5, 3, 7, 9, 34, 3, 7, 8, 5, 4, 3, 9, 9, 6, 6 };
		ShellsSort(A);
	}

}
