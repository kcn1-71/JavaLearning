public class Permutations {

	public static void Permut(String rez, String source) {
		String src = source;
		String rz = rez;
		int ls = source.length();
		if (ls > 0) {
			for (int i = 0; i < ls; i++) {
				rz = rez + source.charAt(i);
				src = source.substring(i + 1, ls);
				Permut(rz, src);
			}

		} else {
			System.out.print(rz + "\n");
		}

	}

	public static void main(String[] args) {
		String rez = "";
		String source = "1234";
		Permut(rez, source);

	}

}
