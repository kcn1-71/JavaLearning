import java.util.Scanner;

public class Triangle 
{

	
	public static long C (long n, long k)
	{
	  if (k == 0 || n == k)
	    return 1;
	  return C (n - 1, k - 1) + C (n - 1, k);
	}
	
	public static int main(String[] args) 
	{
		Scanner in = new Scanner(System.in);
		long n = in.nextLong();
		long k = in.nextLong();
		
		System.out.print(C(n, k) + " ");
		
		return 0;
	}

}
