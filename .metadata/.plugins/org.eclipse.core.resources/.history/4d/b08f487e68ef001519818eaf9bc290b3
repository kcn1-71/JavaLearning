
public class StringSearch {
	
	public static char[] ArrayOfSymbol;
	public static int[] ArrayOfShifts;
	
	
	public static void ShiftForm(String S)
	{
		boolean flag;
		for (int i = S.length()-2; i >= 0; i--)
        {
            flag = false; //Флаг
            int j = 0; //Обнуление
            int k =0;
            while ((j<k+1)&&(flag == false))
            {
                if (ArrayOfSymbol[j] == S.charAt(i)) flag = true;
                j++;
            }
            if (flag == false)
            {
            	ArrayOfSymbol[k] = S.charAt(i);
            	ArrayOfShifts[k] = S.length() - i - 1;
                k++;
            }
        }
	}

	public static void main(String[] args) {
		
		ShiftForm("qwerqwe");
	}

}
